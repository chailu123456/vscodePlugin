{
	"name": "quick-push-code",
	"displayName": "quick-push-code",
	"description": "快速推送代码到远程仓库",
	"version": "1.0.6",
	"repository": {
		"private": true
	},
	"publisher": "LuChai",
	"engines": {
		"vscode": "^1.59.0"
	},
	"icon": "git.jpg",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:quick-push-code.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "quick-push-code.helloWorld",
				"title": "一键推送代码"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorFocus",
					"command": "quick-push-code.helloWorld",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"dependencies": {
		"colors": "^1.4.0",
		"shelljs": "^0.8.4"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/shelljs": "^0.8.9",
		"@types/vscode": "^1.59.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"node": "^16.6.1",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}
